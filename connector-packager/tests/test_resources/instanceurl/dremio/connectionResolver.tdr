<tdr class='dremio'>
  <connection-resolver>
    <connection-builder>
      <script file='connectionBuilder.js'/>
    </connection-builder>
    <connection-normalizer>
      <required-attributes>
        <setImpersonateAttributes/>
        <default-attributes>
          <default-attribute name='server'>sql.dremio.cloud</default-attribute>
          <default-attribute name='port'>443</default-attribute>
        </default-attributes>
        <attribute-list>
          <attr>class</attr>
          <attr>server</attr>
          <attr>port</attr>
          <attr>authentication</attr>
          <attr>username</attr>
          <attr>password</attr>
          <attr>sslmode</attr>
          <attr>v-project-id</attr>
          <attr>v-engine</attr>
          <attr>v-routing-tag</attr>
          <attr>v-routing-queue</attr>
          <attr>v-dremio-product</attr>
        </attribute-list>
      </required-attributes>
    </connection-normalizer>
    <connection-properties>
      <script file='connectionProperties.js'/>
    </connection-properties>
  </connection-resolver>
</tdr>
